class Solution {
public:
    string longestCommonPrefix(vector<string>& strs) {
        string prefix = '';
        for (i = 0; i< strs[0].length(); i++) {
            for (j = 1; i< strs.length(); i++) {
                if (strs[0][i] != strs[j][i]){
                    return prefix;
                }
            }
        }
        return prefix;
        
    }
};
